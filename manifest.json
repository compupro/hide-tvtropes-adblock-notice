{

  "manifest_version": 2,
  "name": "Hide TV Tropes Adblock Notice",
  "homepage_url": "https://github.com/compupro/hide-tvtropes-adblock-notice",
  "author": "compupro",
  "version": "1.0",

  "description": "Removes the whitelist on adblock notice from TV Tropes",

  "applications": {
    "gecko": {
      "id": "hide-tvtropes-adblock-notice@github.com",
      "strict_min_version": "42.0"
    }
  },
  
  "content_scripts": [
    {
      "matches": ["*://*.tvtropes.org/*"],
      "js": ["extension.js"]
    }
  ]

}